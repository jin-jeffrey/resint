{
    "name": "resint",
    "description": "Track Job Applications",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs", "*://*/*"],
    "action": {
        "default_popup": "login.html",
        "default_icon": {
            "16": "/images/leaf_16.png",
            "32": "/images/leaf_32.png",
            "48": "/images/leaf_48.png",
            "128": "/images/leaf_128.png"
          }
    },
    "host_permissions": [
      "*://*/*"
    ],
    "icons": {
      "16": "/images/leaf_16.png",
      "32": "/images/leaf_32.png",
      "48": "/images/leaf_48.png",
      "128": "/images/leaf_128.png"
    }
}